  {
    "udt_struct_v0": {
      "doc": "The governor settings for managing proposals",
      "lib": "",
      "name": "GovernorSettings",
      "fields": [
        {
          "doc": "Determine which votes to count against the quorum out of for, against, and abstain. The value is encoded\\nsuch that only the last 3 bits are considered, and follows the structure `MSB...{against}{for}{abstain}`,\\nsuch that any value != 0 means that type of vote is counted in the quorum. For example, consider\\n5 == `0x0...0101`, this means that votes \"against\" and \"abstain\" are included in the quorum, but votes\\n\"for\" are not.",
          "name": "counting_type",
          "type_": "u32"
        },
        {
          "doc": "The time (in ledgers) the proposal has to be executed before it expires. This starts after the timelock.",
          "name": "grace_period",
          "type_": "u32"
        },
        {
          "doc": "The votes required to create a proposal.",
          "name": "proposal_threshold",
          "type_": "i128"
        },
        {
          "doc": "The percentage of votes (expressed in BPS) needed of the total available votes to consider a vote successful.",
          "name": "quorum",
          "type_": "u32"
        },
        {
          "doc": "The time (in ledgers) the proposal will have to wait between vote period closing and execution.",
          "name": "timelock",
          "type_": "u32"
        },
        {
          "doc": "The delay (in ledgers) from the proposal creation to when the voting period begins. The voting\\nperiod start time will be the checkpoint used to account for all votes for the proposal.",
          "name": "vote_delay",
          "type_": "u32"
        },
        {
          "doc": "The time (in ledgers) the proposal will be open to vote against.",
          "name": "vote_period",
          "type_": "u32"
        },
        {
          "doc": "The percentage of votes \"yes\" (expressed in BPS) needed to consider a vote successful.",
          "name": "vote_threshold",
          "type_": "u32"
        }
      ]
    }
  },
