    "udt_union_v0": {
      "doc": "Curve types",
      "lib": "",
      "name": "Curve",
      "cases": [
        {
          "tuple_v0": {
            "doc": "Constan curve, it will always have the same value",
            "name": "Constant",
            "type_": [
              "u128"
            ]
          }
        },
        {
          "tuple_v0": {
            "doc": "Linear curve that grow linearly but later\\ntends to a constant saturated value.",
            "name": "SaturatingLinear",
            "type_": [
              {
                "udt": {
                  "name": "SaturatingLinear"
                }
              }
            ]
          }
        },
        {
          "tuple_v0": {
            "doc": "Curve with different slopes",
            "name": "PiecewiseLinear",
            "type_": [
